<%- include("headTag") -%>
<%- include('pageTitle'); -%>

<div class="formBox">
  <form class="item" action="/" method="post">
    <!-- <input type="text" name="newItems" placeholder="New task" autocomplete="off"/> -->
    <button type="submit" name="newItems" onclick="setTime();">Timestamp</button>
    <input id="field" type="text" name="field" value="" size="11" />
