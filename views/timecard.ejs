  <% for (let i=0; i<newListItems.length; i++) { %>
  <div class="item">
    <input type="checkbox">
    <p><%= newListItems[i]  %></p>
  </div>
  <% } %>
</div>

  <script src="js/modules.js" charset="utf-8"></script>

